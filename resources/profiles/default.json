{
    "profileID": "default",
    "excludeExtensions": {
        "image_extensions": [".tif", ".tiff", ".bmp", ".jpg", ".jpeg", ".gif", ".png", ".epg", ".raw", ".cr2", ".nef", ".orf", ".sr2"],
        "other": [".json"]
    },
    "excludeFiles": {
        "node_files": ["package-lock.json", "package.json"]
    },
    "moduleDirectories": {
        "directories": ["node_modules", ".git"]
    },
    "excludeMatch": {
        "name": ["John"],
        "email": ["john.doe@mail.com"],
        "ip": ["123.123.123.123"],
        "password": ["password"],
        "key": ["asdfgh1231jkasduy%asd"]
    },
    "includeMatch": {
        "name": ["bestnameever"],
        "email": ["super.awesome@mail.com"],
        "ip": ["321.321.321.321"],
        "password": ["hardpassword"],
        "key": ["somekey"]
    }
}